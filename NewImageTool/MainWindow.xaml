<Window x:Class="NewImageTool.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" >
    <Grid>
        <Ellipse Height="20" Fill="Red" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="10,5,0,0" Name="ell_flag" Stroke="Red" Width="20" />
        <Image x:Name="cam" HorizontalAlignment="Left" Height="300" Margin="10,30,0,0" VerticalAlignment="Top" Width="400"/>
        <Image x:Name="cam_right" HorizontalAlignment="Left" Height="300" Margin="415,30,-55,0" VerticalAlignment="Top" Width="400"/>
        <Button Content="Button" HorizontalAlignment="Left" VerticalAlignment="Top" Width="50" Height="30" Margin="10,350,0,10" Click="Button_Click"/>
        <Label x:Name="lbtime" Content="Label" HorizontalAlignment="Left" Margin="415,350,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lbarea" Content="Label" HorizontalAlignment="Left" Margin="415,391,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.421,1.077"/>
    </Grid>
</Window>
